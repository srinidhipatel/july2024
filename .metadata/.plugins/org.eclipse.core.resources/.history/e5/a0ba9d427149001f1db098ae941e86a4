
public class CondIFElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String signalColor="GREEN";
		if(signalColor == "RED") {
			System.out.println("stop the car");
			System.out.println("Stop the car");
			System.out.println("Stop the car");
			System.out.println("Stop the car");
			System.out.println("Stop the car");
		}else {
			System.out.println("Start my car");
		}
		
		/*
		 *  if(condetion is true){
		 *  execute if statements
		 *  statement 1
		 *  state
		 *  }else{ // If the condetion is failed then it will go to else blocks
		 *  Statement 1
		 *  Stement 2
		 *  
		 *  }
		 *  
		 *  
		 *  if you are scoring above 35 --> Pass
		 *   below 35 --> Fail
		 *   
		 *  
		 * 
		 * */
		
		int marks = 15;
		if(marks >35) {
			System.out.println("Student is passed");
		}else {
			System.out.println("Student is failed");
		}
		
	}

}
